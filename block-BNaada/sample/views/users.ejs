<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h4>
        listing users
    </h4>
    <% users.forEach(user => {%> 
        <section>
            <p>
               <%= user.name %>
            </p>
            <p>
               <%= user.email %>
            </p>
            <a href="/users/<%= user.id %>">
                <button>
                    show
                </button>
            </a>
        </section>
    <% }) %>
</body>
</html>